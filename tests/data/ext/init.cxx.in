#include "${module}.h"

static PyMethodDef ${module}_methods[] = {
  %for method in methods:
  { "${method}", ${module}_${method}, METH_VARARGS, nullptr },
  %endfor
  { nullptr, nullptr, 0, nullptr }
};

static PyModuleDef ${module} = {
  PyModuleDef_HEAD_INIT,
  "${module}",
  nullptr,
  -1,
  ${module}_methods
};

PyMODINIT_FUNC PyInit_${module} () { return PyModule_Create(&${module}); }
